    },
    "homepage": "https://github.com/azappella/solid-minimal#readme",
    "devDependencies": {
        "@babel/cli": "^7.4.3",
        "@babel/core": "^7.4.3",
        "@babel/preset-env": "^7.3.1",
        "babel-jest": "^24.0.0",
        "babel-loader": "^8.0.4",
        "babel-preset-solid": "^0.1.0",
        "clean-webpack-plugin": "^1.0.1",
        "cross-env": "^5.2.0",
        "css-loader": "^2.1.0",
        "dotenv-webpack": "^1.7.0",
